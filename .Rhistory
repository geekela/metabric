square <- function(x) {
result <- x^2        # calculate the square
print(result)        # display it on the screen
}
square(0)
square(3)
square(52.3)
# Loop from 4 to 30
for (i in 4:30) {
square <- i^2
print(square)
}
for (i in 4:30) {
if (i %% 2 == 0) {        # check if number is even
print(i^2)
}
}
square <- function(x) {
x^2
}
nums <- 4:30
squares <- sapply(nums, square)
print(squares)
cat("Length:", length(squares), "\n")
cat("Sum:",    sum(squares),    "\n")
cat("Min:",    min(squares),    "\n")
cat("Max:",    max(squares),    "\n")
cat("Average:", mean(squares),  "\n")
cat("Median:", median(squares), "\n")
imin <- which.min(squares)
imax <- which.max(squares)
cat("Index of min:", imin,
"| value:", squares[imin],
"| original number:", nums[imin], "\n")
cat("Index of max:", imax,
"| value:", squares[imax],
"| original number:", nums[imax], "\n")
